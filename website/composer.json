  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.8",
    "vlucas/phpdotenv": "^4.1.8",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/wordpress": "^5.8.2",
    "roots/wp-config": "^1.0.0",
    "roots/wp-password-bcrypt": "^1.0.0",
    "advanced-custom-fields/advanced-custom-fields-pro": "^5.9",
    "wpackagist-plugin/cookie-notice": "^2.2.1",
    "wpackagist-plugin/post-types-order": "^1.9.5.2",
    "wpackagist-plugin/acf-extended": "^0.8.7.4",
    "wpackagist-plugin/contact-form-7": "^5.4.1",
    "wpackagist-plugin/timber-library": "^1.19.1",
    "wpackagist-plugin/w3-total-cache": "^2.2.1",
    "wpackagist-plugin/wp-migrate-db": "^2.2.1",
    "wpackagist-plugin/advanced-custom-fields-contact-form-7-field": "^1.1.2",
    "wpackagist-plugin/regenerate-thumbnails": "^3.1.5",
    "wpackagist-plugin/wordpress-seo": "^20",
    "wpackagist-plugin/relevanssi": "^4.14.4",
    "wpackagist-plugin/folders": "^2.7.4",
    "drewm/mailchimp-api": "^2.5",
    "wpml/wpml-multilingual-cms": "4.5.2",
    "wpml/wpml-string-translation": "3.2.0",
    "wpml/wpml-cf7-multilangual": "1.1.0",
    "wpml/wpml-acf-multilangual": "1.9.5",
    "wpml/wpml-yoast-multilangual": "2.0.1",
    "wpackagist-plugin/duplicate-page": "^4.4.1",
    "wpackagist-plugin/duplicate-post": "^4.1.2",
    "wpackagist-plugin/wpcf7-recaptcha": "^1.3.7",
    "wpackagist-plugin/leadin": "^10",
    "wpackagist-plugin/contact-form-7-modules": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.6",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pivvenit/acf-pro-installer": true,
      "ffraenz/private-composer-installer": true,
      "composer/installers": true,
      "roots/wordpress-core-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
